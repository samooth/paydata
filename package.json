{
  "name": "paydata",
  "version": "0.3.7",
  "description": "post data over bitcoin sv",
  "main": "index.js",
  "scripts": {
    "test": "npx mocha",
    "generate-docs": "jsdoc --configure jsdoc.json --verbose -R README.md index.js",
    "build": "npx browserify index.js --standalone paydata > dist/paydata.js && terser --compress --mangle -- dist/paydata.js > dist/paydata.min.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "fetch": false,
    "dotenv": false
  },
  "keywords": [
    "bsv",
    "bitcoinsv",
    "bitcoin",
    "blockchain",
    "cryptocurrency"
  ],
  "author": "@samooth",
  "license": "MIT",
  "unpkg": "dist/paydata.min.js",
  "dependencies": {
    "bsv": "github:samooth/bsv",
    "buffer": "^6.0.3",
    "form-data": "^4.0.0",
    "mingo": "^6.1.0",
    "whatsonchain": "github:samooth/whatsonchain"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "clean-jsdoc-theme": "^4.1.8",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "mocha": "^10.0.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "uglify-es": "^3.3.9",
    "util": "^0.12.4"
  }
}
